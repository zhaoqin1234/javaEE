package com.hrxt.pojo;

/**
 * 措施计划  下拉框  option bean
 * 
 * @author ZhaoQin
 *
 */
public class CsjhOption {

	private String cs_id;			//措施ID
	private String cs_name;			//措施名称
	private String cs_type;			//措施类型（0：常规措施类型；1：重点措施类型）
	private String cs_flag;			//标记（0:措施公用 1油井措施 2水井措施）
	private String sort_order;		//排序
	private String start_using;		//是否启用 Y 启用  N 不启用
	
	public String getCs_id() {
		return cs_id;
	}
	public void setCs_id(String cs_id) {
		this.cs_id = cs_id;
	}
	public String getCs_name() {
		return cs_name;
	}
	public void setCs_name(String cs_name) {
		this.cs_name = cs_name;
	}
	public String getCs_type() {
		return cs_type;
	}
	public void setCs_type(String cs_type) {
		this.cs_type = cs_type;
	}
	public String getCs_flag() {
		return cs_flag;
	}
	public void setCs_flag(String cs_flag) {
		this.cs_flag = cs_flag;
	}
	public String getSort_order() {
		return sort_order;
	}
	public void setSort_order(String sort_order) {
		this.sort_order = sort_order;
	}
	public String getStart_using() {
		return start_using;
	}
	public void setStart_using(String start_using) {
		this.start_using = start_using;
	}



	

}
